{
  "name": "webpack-react-config",
  "version": "0.0.1",
  "private": true,
  "description": "A bare, minimal of React Web App with Webpack without any configuration",
  "module": "dist/webpack.mjs",
  "main": "dist/webpack.js",
  "exports": {
    ".": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    }
  },
  "homepage": "https://github.com/hoangvvo/webpack-react-config#readme",
  "bugs": {
    "url": "https://github.com/hoangvvo/webpack-react-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoangvvo/webpack-react-config.git"
  },
  "license": "ISC",
  "author": "",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bundt && cp src/dirname.cjs dist/dirname.cjs"
  },
  "modes": {
    "default": "src/webpack.js"
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "copy-webpack-plugin": "^9.0.1",
    "html-webpack-plugin": "^5.3.2",
    "react-refresh": "^0.10.0",
    "source-map-loader": "^3.0.0",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "bundt": "^1.1.5",
    "fs-extra": "^10.0.0",
    "kleur": "^4.1.4"
  },
  "peerDependencies": {
    "webpack": "^5.0.0",
    "babel-loader": "^8.2.2",
    "swc-loader": "^0.1.15"
  },
  "peerDependenciesMeta": {
    "babel-loader": {
      "optional": true
    },
    "swc-loader": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
